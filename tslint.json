{
	"defaultSeverity": "warning",
	"extends": ["tslint:recommended"],
	"linterOptions": {
		"exclude": [
			"node_modules",
			"**/mixifier/**",
			"./mixifier",
			"mixifier",
			"/mixifier",
			"~mixifier",
			"./mixifier/buttons/Zoom.html",
			"./mixifier/buttons/*"
		]
	},
	"rules": {
		"quotemark": [true, "single"],
		"indent": [true, "tabs", 4],
		"interface-name": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false
	}
}
